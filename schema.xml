<?xml version="1.0"?>
<rdf:RDF xmlns:space="http://www.spacerdf.org/schema.xml#" xmlns:spaceweb="http://www.spacerdf.org/" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:time="http://www.w3.org/2006/time#">
	<owl:Ontology rdf:about="http://www.spacerdf.org/schema.xml#">
		<dc:title>SpaceRDF Vocabulary</dc:title>
		<rdfs:comment>RDF vocabulary for describing hackerspaces and related concepts</rdfs:comment>
		<owl:imports rdf:resource="http://xmlns.com/foaf/0.1/" />
	</owl:Ontology>
	<owl:Class rdf:about="http://www.spacerdf.org/schema.xml#Space">
		<rdfs:comment>A space is a physical place, where people can meet and work
                    on their projects. Currently, no effort is made to seperate
                    the physical space from the group of people that use it or
                    any formal organisation structure.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group" />
		<owl:disjointWith>
			<owl:Class rdf:about="http://www.spacerdf.org/schema.xml#State">
				<rdfs:comment>The state of a hackerspace. The validity of instances of 
                  this class is time-limited.</rdfs:comment>
				<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
				<owl:disjointWith rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
				<owl:disjointWith rdf:resource="http://www.spacerdf.org/schema.xml#SpaceAPIDocument" />
			</owl:Class>
		</owl:disjointWith>
		<owl:disjointWith>
			<owl:Class rdf:about="http://www.spacerdf.org/schema.xml#SpaceAPIDocument">
				<rdfs:comment>A SpaceAPI JSON document found at a SpaceAPI endpoint.</rdfs:comment>
				<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
				<rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Document" />
				<owl:disjointWith rdf:resource="http://www.spacerdf.org/schema.xml#State" />
				<owl:disjointWith rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
			</owl:Class>
		</owl:disjointWith>
	</owl:Class>
	<owl:DatatypeProperty rdf:about="http://www.spacerdf.org/schema.xml#name">
		<rdfs:comment>The name of the hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/name" />
	</owl:DatatypeProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#logo">
		<rdfs:comment>A URL for the hackerspace's logo.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Image" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/logo" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#url">
		<rdfs:comment>The URL of the hackerspace's homepage.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/homepage" />
	</owl:ObjectProperty>
	<owl:DatatypeProperty rdf:about="http://www.spacerdf.org/schema.xml#address">
		<rdfs:comment>The postal address of the hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
	</owl:DatatypeProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#location">
		<rdfs:comment>The geographical location of the hackersapce.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#state">
		<rdfs:comment>A recorded state of the hackerspace. Objects referenced
                    will not necessarily be the latest state, and likely
                    won't be. The latest state can be determined by the
                    space:changed field of the space:State.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://www.spacerdf.org/schema.xml#State" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#spaceAPI">
		<rdfs:comment>A link to the SpaceAPI endpoint for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://www.spacerdf.org/schema.xml#SpaceAPIDocument" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#phone">
		<rdfs:comment>A tel: URI for contacting the hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/phone" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#sip">
		<rdfs:comment>A sip: URI for contacting the hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/phone" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#irc">
		<rdfs:comment>An irc: or ircs: URI for the hackerspace's IRC channel</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#twitter">
		<rdfs:comment>The Twitter account for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#facebook">
		<rdfs:comment>The Facebook account for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#googleplus">
		<rdfs:comment>The Google Plus account for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#identica">
		<rdfs:comment>The identica account for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#foursquare">
		<rdfs:comment>The foursquare account for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#email">
		<rdfs:comment>A contact email address for the hackerspace. If this
                    address is a mailing list, consider using space:ml
                    instead.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
		<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/mbox" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#ml">
		<rdfs:comment>A mailing list for a hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#jabber">
		<rdfs:comment>An XMPP conference address for the hackerspace.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#Space" />
	</owl:ObjectProperty>
	<owl:DatatypeProperty rdf:about="http://www.spacerdf.org/schema.xml#open">
		<rdfs:comment>Boolean value reflecting whether or not the space is open or closed for a given State.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#State" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean" />
	</owl:DatatypeProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#triggerPerson">
		<rdfs:comment>A foaf:PersonalProfileDocument for the person that triggered the State change.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#State" />
		<rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/PersonalProfileDocument" />
	</owl:ObjectProperty>
	<owl:DatatypeProperty rdf:about="http://www.spacerdf.org/schema.xml#triggeredBy">
		<rdfs:comment>The name, in the loosest sense of the word, of the person that triggered the State change.</rdfs:comment>
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#State" />
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
	</owl:DatatypeProperty>
	<owl:ObjectProperty rdf:about="http://www.spacerdf.org/schema.xml#changed">
		<rdfs:comment>The time at which the state was changed.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml" />
		<rdfs:domain rdf:resource="http://www.spacerdf.org/schema.xml#State" />
		<rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant" />
	</owl:ObjectProperty>
</rdf:RDF>

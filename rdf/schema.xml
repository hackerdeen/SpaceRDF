<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns="http://www.spacerdf.org/schema#">
  <rdf:Description rdf:about="http://www.spacerdf.org/schema.xml#">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema.xml#">
    <dc:title>SpaceRDF Vocabulary</dc:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema.xml#">
    <rdfs:comment>RDF vocabulary for describing hackerspaces and related concepts</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema.xml#">
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#Space">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#Space">
    <rdfs:comment>A space is a physical place, where people can meet and work on their projects. Currently, no effort is made to seperate the physical space from the group of people that use it or any formal organisation structure.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#Space">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#Space">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#Space">
    <owl:disjointWith rdf:resource="http://www.spacerdf.org/schema#State"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#SpaceAPIDocument">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#SpaceAPIDocument">
    <rdfs:comment>A SpaceAPI JSON document found at a SpaceAPI endpoint.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#SpaceAPIDocument">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#SpaceAPIDocument">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#State">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#State">
    <rdfs:comment>The state of a hackerspace. The validity of instances of this class is time-limited.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#State">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#email">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#email">
    <rdfs:comment>A contact email address for the hackerspace. If this address is a mailing list, consider using space:ml instead.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#email">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#email">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#email">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/mbox"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdfs:comment>A URL for the hackerspace's logo.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Image"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#logo">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/logo"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#ml">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#ml">
    <rdfs:comment>The primary mailing list for a hackerspace.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#ml">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#ml">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdfs:comment>The name of the hackerspace.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#name">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/name"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#open">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#open">
    <rdfs:comment>Boolean value reflecting whether or not the space is open or closed for a given State.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#open">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#State"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#open">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#open">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#phone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#phone">
    <rdfs:comment>Either a tel:, sip: or h323: URI for contacting the hackerspace with voice (and maybe video).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#phone">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#phone">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#phone">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/phone"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#spaceAPI">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#spaceAPI">
    <rdfs:comment>A link to the SpaceAPI endpoint for a hackerspace.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#spaceAPI">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#spaceAPI">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#spaceAPI">
    <rdfs:range rdf:resource="http://www.spacerdf.org/schema#SpaceAPIDocument"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggerPerson">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggerPerson">
    <rdfs:comment>A foaf:PersonalProfileDocument for the person that triggered the State change.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggerPerson">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#State"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggerPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggerPerson">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/PersonalProfileDocument"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggeredBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggeredBy">
    <rdfs:comment>The name, in the loosest sense of the word, of the person that triggered the State change.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggeredBy">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#State"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggeredBy">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#triggeredBy">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdfs:comment>The Twitter account for a hackerspace.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#twitter">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/account"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdfs:comment>The URL of the hackerspace's homepage.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdfs:domain rdf:resource="http://www.spacerdf.org/schema#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdfs:isDefinedBy rdf:resource="http://www.spacerdf.org/schema.xml"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.spacerdf.org/schema#url">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/homepage"/>
  </rdf:Description>
</rdf:RDF>
